#include<bits/stdc++.h>

using namespace std;
int main()
{
    int a[10]={0,1,5,10,2,5};
    int m=0;
	int sum=0;
	for(int i=1;i<=5;i++)sum+=a[i];//计算总时长 
	m=sum/2;//因为有两个机器
	int p[10][10]={0};
	for(int i=1;i<=5;i++)
	for(int j=1;j<=m;j++)
	{
		if(j-a[i]>=0)p[i][j]=max(p[i-1][j], p[i-1][j-a[i]]+a[i]);
		else p[i][j]=p[i-1][j];
		cout<<i<<" "<<j<<" "<<p[i][j]<<endl;
	 } 
	 cout<<sum-p[5][m];
    
}

